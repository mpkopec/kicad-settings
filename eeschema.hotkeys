shortcut   "Ctrl+C":    "Save Block"
shortcut   "M":    "Move Schematic Item"
shortcut   "C":    "Copy Component or Label"
shortcut   "A":    "Add Component"
shortcut   "P":    "Add Power"
shortcut   "X":    "Mirror X"
